        <record id="facturas_sales_compras_sequence" model="ir.sequence">
            <field name="name">Facturas Sales Compras</field>
            <field name="code">facturas.sales_compras</field>
            <field name="prefix">COM/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>