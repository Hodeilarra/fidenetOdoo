<?xml version="1.0"?>
<odoo>
    <data>
        <record id="sale_order_nombre_obra_form_view" model="ir.ui.view">
            <field name="name">sale.order.from.inherited.nombreObra</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="nombreObra"/>
                </xpath>
                
            </field>
        </record>
    </data>
</odoo>
 
