<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_stock_barcodes_read_picking_form_inherit" model="ir.ui.view">
        <field name="name">view.stock.barcodes.read.picking.form.inherit</field>
        <field name="model">wiz.stock.barcodes.read.picking</field>
        <field name="inherit_id" ref="stock_barcodes.view_stock_barcodes_read_picking_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='confirmed_moves']" position="after">
                <label for="is_qr">
                    Is a QR Code
                </label>
                <field name="is_qr" widget="barcode_boolean_toggle" />
                <label for="allow_duplicated">
                    Allow duplicated codes
                </label>
                <field name="allow_duplicated" widget="barcode_boolean_toggle" />
            </xpath>
        </field>
    </record>

</odoo>
